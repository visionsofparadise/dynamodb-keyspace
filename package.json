{
	"name": "dynamodb-keyspace",
	"description": "Commands and utilities for better, more scalable DynamoDB usage.",
	"version": "0.0.1",
	"main": "./dist-cjs/index.js",
	"module": "./dist-es/index.js",
	"types": "./dist-types/index.d.ts",
	"files": [
		"dist-*/**"
	],
	"sideEffects": false,
	"scripts": {
		"check": "tsc --noEmit",
		"build": "concurrently 'npm:build:cjs' 'npm:build:es' 'npm:build:types'",
		"build:cjs": "tsc -p tsconfig.cjs.json",
		"build:es": "tsc -p tsconfig.es.json",
		"build:types": "tsc -p tsconfig.types.json",
		"test": "jest",
		"patch": "npm version patch && npm publish",
		"cdk": "cdk"
	},
	"devDependencies": {
		"@aws-sdk/client-dynamodb": "^3.369.0",
		"@aws-sdk/lib-dynamodb": "^3.369.0",
		"@aws-sdk/util-dynamodb": "^3.370.0",
		"@shelf/jest-dynamodb": "^3.4.1",
		"@types/jest": "^27.0.3",
		"@types/lodash": "^4.14.171",
		"@types/node": "^18.11.5",
		"aws-cdk": "^2.44.0",
		"aws-cdk-lib": "^2.44.0",
		"concurrently": "^8.2.0",
		"constructs": "^10.1.122",
		"dayjs": "^1.11.9",
		"jest": "^29.6.1",
		"rimraf": "^5.0.1",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"ts-toolbelt": "^9.6.0",
		"tslint": "^5.20.1",
		"tslint-config-prettier": "^1.18.0",
		"tslint-plugin-prettier": "^2.1.0",
		"typescript": "^5.1.6"
	},
	"peerDependencies": {
		"@aws-sdk/client-dynamodb": "^3.369.0",
		"@aws-sdk/lib-dynamodb": "^3.369.0"
	},
	"author": {
		"name": "xKore",
		"url": "https://www.mattcavender.com"
	},
	"license": "MIT",
	"homepage": "https://github.com/visionsofparadise/dynamodb-keyspace#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/visionsofparadise/dynamodb-keyspace.git"
	},
	"bugs": {
		"url": "https://github.com/visionsofparadise/dynamodb-keyspace/issues"
	}
}
